{

  "manifest_version": 2,
  "name": "Passme",
  "version": "1.0",

  "description": "Add a button on every password input to allow explore the password written.",

  "content_scripts": [
    {
      "all_frames": true,
      "matches": ["*://*/*"],
      "js": ["script.js"]   
    }
  ],
  "web_accessible_resources": [
    "icons/*"
  ]
}